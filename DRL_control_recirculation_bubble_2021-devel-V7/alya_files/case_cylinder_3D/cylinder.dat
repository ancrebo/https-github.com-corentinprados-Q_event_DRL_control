    END_PARTITIONING
  END_PARALL_SERVICE 
END_PROBLEM_DATA
$-------------------------------------------------------------------
MPI_IO: ON
  GEOMETRY: ON $ Export
  RESTART: ON
  POSTPROCESS: ON
END_MPI_IO
